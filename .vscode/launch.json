{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Jest E2E",
        "program": "${workspaceFolder}/node_modules/.bin/jest",
        "args": [
          "--config",
          "test/jest-e2e.json",
          "--runInBand"
        ],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  